@model IEnumerable<comunidadecloud.Models.Alunos>

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Lista de Alunos</h1>
</div>

<table class="table">
    <thead>
        <tr>
            <th scope="col">#Id</th>
            <th scope="col">Nome</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.id</td>
                <td>@item.name</td>
            </tr>
        }
    </tbody>
</table>