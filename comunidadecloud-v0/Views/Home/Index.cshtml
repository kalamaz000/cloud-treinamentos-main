@model IEnumerable<comunidadecloud.Models.Blobs>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Galeria</h1>
</div>

<div class="container" style="display: flex;">
    @foreach (var item in Model)
    {
        <div class="card" style="width: 200px; height: 200px; display: inline-block; margin: 5px">
        <img class="card-img-top" src="@item.URL" alt="Card image cap">
        </div>
    }
</div>